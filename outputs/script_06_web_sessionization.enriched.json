  },
  "bedrock_enrichment": {
    "group_by": [
      {
        "df": "sessions",
        "keys": [
          "customer_id",
          "session_id"
        ]
      }
    ],
    "aggregations": [
      {
        "df": "sessions",
        "expr": "MIN(event_ts)",
        "source_cols": [
          "event_ts"
        ],
        "alias": "session_start_ts"
      },
      {
        "df": "sessions",
        "expr": "MAX(event_ts)",
        "source_cols": [
          "event_ts"
        ],
        "alias": "session_end_ts"
      },
      {
        "df": "sessions",
        "expr": "COUNT(1)",
        "source_cols": [
          "*"
        ],
        "alias": "events"
      },
      {
        "df": "sessions",
        "expr": "COUNT(DISTINCT url_host)",
        "source_cols": [
          "url_host"
        ],
        "alias": "unique_hosts"
      }
    ],
    "derived_columns": [
      {
        "df": "clicks",
        "col": "event_ts",
        "expression": "to_timestamp(event_ts)",
        "source_cols": [
          "event_ts"
        ]
      },
      {
        "df": "clicks",
        "col": "event_date",
        "expression": "to_date(event_ts)",
        "source_cols": [
          "event_ts"
        ]
      },
      {
        "df": "clicks",
        "col": "url_host",
        "expression": "regexp_extract(url, 'https?://([^/]+)/', 1)",
        "source_cols": [
          "url"
        ]
      },
      {
        "df": "clicks",
        "col": "ref_host",
        "expression": "regexp_extract(referrer, 'https?://([^/]+)/', 1)",
        "source_cols": [
          "referrer"
        ]
      },
      {
        "df": "df2",
        "col": "prev_ts",
        "expression": "lag(event_ts).over(w)",
        "source_cols": [
          "event_ts"
        ]
      },
      {
        "df": "df3",
        "col": "gap_min",
        "expression": "(col(event_ts).cast('long') - col(prev_ts).cast('long')) / 60.0",
